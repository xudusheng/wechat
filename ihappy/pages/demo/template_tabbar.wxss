/* pages/demo/template_tabbar.wxss */
.bottomposition{
    width: 100%;
    height: 10%;
    position: fixed;
    overflow: hidden;
    left: 0;
    top: 90%;
z-index: 1100;
    display: flex;
    border-top: 1rpx solid #dadada;
}
.item_info {
    width: 25%;
    height: 100%;
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
}
.item_info image {
    width: 20px;
    height: 20px;
}
.item_info text {
    margin-top: 5px;
    font-size: 12px;
}